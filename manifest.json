{
    "manifest_version": 2,
    "name": "Dark Mode for Overcast",
    "description": "Adds dark mode to overcast.fm",
    "version": "0.1",
    "browser_action": {
        "default_icon": {
            "16": "icon.png",
            "32": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://*.overcast.fm/*"],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": ["night.css"]
}